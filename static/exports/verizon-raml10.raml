#%RAML 1.0
title: Verizon
version: '1.3'
baseUri: https://5gedge.verizon.com/api/mec/eds
baseUriParameters: {}
protocols:
- HTTPS
documentation:
- title: Verizon
  content: The Verizon Edge Discovery Service API can direct your application clients to connect to the optimal service endpoints for your Multi-access Edge Computing (MEC) applications for every session. The Edge Discovery Service takes into account the current location of a device, its IP anchor location, current network traffic and other factors to determine which 5G Edge platform a device should connect to.
securitySchemes:
  thingspace_oauth:
    type: OAuth 2.0
    settings:
      authorizationUri: ''
      accessTokenUri: https://thingspace.verizon.com/api/ts/v1/oauth2/token
      authorizationGrants:
      - client_credentials
      scopes:
      - discovery:read
      - serviceprofile:read
      - serviceprofile:write
      - serviceregistry:read
      - serviceregistry:write
      - ts.mec.fullaccess
      - ts.mec.limitaccess
      - ts.application.ro
      - edge:discovery:read
      - edge:serviceprofile:read
      - edge:serviceprofile:write
      - edge:serviceregistry:read
      - edge:serviceregistry:write
      - read
      - write
  VZ-M2M-Token:
    type: Pass Through
    description: M2M Session Token ([How to generate an M2M session token?](page:getting-started/5g-edge-developer-creds-token#obtaining-a-vz-m2m-sessiontoken-programmatically))
    describedBy:
      headers:
        VZ-M2M-Token:
          required: true
          displayName: VZ-M2M-Token
          description: M2M Session Token ([How to generate an M2M session token?](page:getting-started/5g-edge-developer-creds-token#obtaining-a-vz-m2m-sessiontoken-programmatically))
          type: string
securedBy:
- thingspace_oauth:
    scopes:
    - edge:discovery:read
    - edge:serviceprofile:read
    - edge:serviceprofile:write
    - edge:serviceregistry:read
    - edge:serviceregistry:write
    - ts.application.ro
    - ts.mec.fullaccess
    - ts.mec.limitaccess
- VZ-M2M-Token
types:
  DeleteServiceProfileResult:
    displayName: DeleteServiceProfileResult
    description: Response on successful deletion of service profile.
    type: object
    properties:
      status:
        required: false
        example:
          value: Success
        displayName: status
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      message:
        required: false
        example:
          value: Service Profile Deleted
        displayName: message
        type: string
        maxLength: 32
  UpdateServiceProfileResult:
    displayName: UpdateServiceProfileResult
    description: Response on successful update of service profile.
    type: object
    properties:
      status:
        required: false
        example:
          value: Success
        displayName: status
        description: HTTP status code.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      message:
        required: false
        example:
          value: Service Profile Updated
        displayName: message
        description: Service Profile that are updated or error details in case of an error.
        type: string
        maxLength: 32
  ListServiceProfilesResult:
    displayName: ListServiceProfilesResult
    description: Response on successful retrieval of service profiles.
    type: object
    properties:
      status:
        required: false
        example:
          value: Success
        displayName: status
        description: HTTP status code.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      data:
        required: false
        displayName: data
        description: A comma delimited list of all the service profiles registered under your API key.
        type: array
        uniqueItems: false
        maxItems: 100
        items:
          type: string
          maxLength: 32
  CreateServiceProfileResult:
    displayName: CreateServiceProfileResult
    description: A successful request returns a serviceProfileId that you can use in subsequent requests.
    type: object
    properties:
      serviceProfileId:
        required: false
        example:
          value: 4054ea9a-593e-4776-b488-697b1bfa4f3b
        displayName: serviceProfileId
        description: Unique identifier for a service profile.
        type: string
  DeregisterServiceEndpointResult:
    displayName: DeregisterServiceEndpointResult
    description: Response to deregister an application's Service Endpoint from one or more Multi-access Edge Compute (MEC) Platforms.
    type: object
    properties:
      status:
        required: false
        example:
          value: Success
        displayName: status
        description: HTTP status code.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      message:
        required: false
        example:
          value: EdgeAppServicesID Deleted
        displayName: message
        description: EdgeAppServicesID that are deleted or error details in case of an error.
        type: string
        maxLength: 32
  UpdateServiceEndpointResult:
    displayName: UpdateServiceEndpointResult
    description: Response to update registered Service Endpoint information.
    type: object
    properties:
      status:
        required: false
        example:
          value: Success
        displayName: status
        description: HTTP status code.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      message:
        required: false
        example:
          value: EdgeAppServices are updated
        displayName: message
        description: EdgeAppServices are updated or error details in case of an error.
        type: string
        maxLength: 64
  ListAllServiceEndpointsResult:
    displayName: ListAllServiceEndpointsResult
    description: Response on successful retrieval of all registered service endpoints.
    type: object
    properties:
      status:
        required: false
        default: success
        displayName: status
        description: HTTP status code.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      data:
        required: false
        displayName: data
        description: A comma delimited list of all registered service endpoints.
        type: array
        uniqueItems: false
        maxItems: 100
        items:
          type: string
          maxLength: 32
  RegisterServiceEndpointResult:
    displayName: RegisterServiceEndpointResult
    description: Response to register Service Endpoints of a deployed application to specified Muli-access Edge Compute (MEC) Platforms.
    type: object
    properties:
      serviceEndpointsId:
        required: false
        example:
          value: 43f3f7bb-d6c5-4bad-b081-9a3a0df2db98
        displayName: serviceEndpointsId
        description: A system-defined string identifier representing one or more registered Service Endpoints.
        type: string
  ListOptimalServiceEndpointsResult:
    displayName: ListOptimalServiceEndpointsResult
    description: Response on successful retrieval of optimal service endpoints for clients.
    type: object
    properties:
      serviceEndpoints:
        required: false
        displayName: serviceEndpoints
        description: An array of optimal Service Endpoint IDs for clients to connect to.
        type: array
        uniqueItems: false
        maxItems: 100
        items:
          type: ResourcesEdgeHostedService
  ListRegionsResult:
    displayName: ListRegionsResult
    description: Response to return an array of all regions in the Verizon 5G Edge service. You can use the region IDs from the response to find optimal Edge platforms or service endpoints.
    type: object
    properties:
      regions:
        required: false
        displayName: regions
        description: An array of all regions in the Verizon 5G Edge service.
        type: array
        uniqueItems: false
        maxItems: 100
        items:
          type: Region
  ListMECPlatformsResult:
    displayName: ListMECPlatformsResult
    description: Response to return the optimal 5G Edge platforms for deployment based on a region, service profile of the service that you want to deploy or user equipment.
    type: object
    properties:
      MECPlatforms:
        required: false
        displayName: MECPlatforms
        description: A list of optimal MEC Platforms where you can register your deployed application.
        type: array
        uniqueItems: false
        maxItems: 100
        items:
          type: MECPlatformResource
  ResourcesServiceProfile:
    displayName: ResourcesServiceProfile
    description: Information about the resource requirements and service characteristics of an edge application. The `maxLatencyMs` and `clientType` parameters are both required in the request body. **Note:** The `maxLatencyMs` value must be submitted in multiples of 5. Does not include serviceProfileId
    type: object
    properties:
      clientType:
        required: true
        displayName: clientType
        description: The category of application client.
        type: string
        enum:
        - V2X
        - ComputerVision
        - MachineLearning
        - IoT
        - Gaming
        - AR
        - VR
        - Analytics
        - Robotics
      ecspFilter:
        required: false
        example:
          value: Verizon
        displayName: ecspFilter
        description: Identity of the preferred Edge Computing Service Provider.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      clientSchedule:
        required: false
        example:
          value: time windows
        displayName: clientSchedule
        description: The expected operation schedule of the application client (e.g. time windows).
        type: string
        pattern: ^[A-Za-z0-9 ]{3,32}$
        maxLength: 32
      clientServiceArea:
        required: false
        example:
          value: BAY AREA
        displayName: clientServiceArea
        description: The expected location(s) (e.g. route) of the hosting UE during the Client's operation schedule.
        type: string
        pattern: ^[A-Za-z0-9 ]{3,32}$
        maxLength: 32
      networkResources:
        required: false
        displayName: networkResources
        description: Network resources of a service profile.
        type: NetworkResourcesType
      computeResources:
        required: false
        displayName: computeResources
        description: Compute resources of a service profile.
        type: ComputeResourcesType
      properties:
        required: false
        displayName: properties
        description: Additional service support information for the MEC platform.
        type: MECPlatformsAdditionalSupportInfo
  ResourcesServiceProfileWithId:
    displayName: ResourcesServiceProfileWithId
    description: Information about the resource requirements and service characteristics of an edge application. Includes serviceProfileId. Used when retrieving a service profile.
    type: object
    properties:
      serviceProfileId:
        required: false
        example:
          value: 4054ea9a-593e-4776-b488-697b1bfa4f3b
        displayName: serviceProfileId
        description: Unique identifier for a service profile.
        type: string
      clientType:
        required: true
        displayName: clientType
        description: The category of application client.
        type: string
        enum:
        - V2X
        - ComputerVision
        - MachineLearning
        - IoT
        - Gaming
        - AR
        - VR
        - Analytics
        - Robotics
      ecspFilter:
        required: false
        example:
          value: Verizon
        displayName: ecspFilter
        description: Identity of the preferred Edge Computing Service Provider.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      clientSchedule:
        required: false
        example:
          value: time windows
        displayName: clientSchedule
        description: The expected operation schedule of the application client (e.g. time windows).
        type: string
        pattern: ^[A-Za-z0-9 ]{3,32}$
        maxLength: 32
      clientServiceArea:
        required: false
        example:
          value: BAY AREA
        displayName: clientServiceArea
        description: The expected location(s) (e.g. route) of the hosting UE during the Client's operation schedule.
        type: string
        pattern: ^[A-Za-z0-9 ]{3,32}$
        maxLength: 32
      networkResources:
        required: false
        displayName: networkResources
        description: Network resources of a service profile.
        type: NetworkResourcesType
      computeResources:
        required: false
        displayName: computeResources
        description: Compute resources of a service profile.
        type: ComputeResourcesType
      properties:
        required: false
        displayName: properties
        description: Additional service support information for the MEC platform.
        type: MECPlatformsAdditionalSupportInfo
  MECPlatformsAdditionalSupportInfo:
    displayName: MECPlatformsAdditionalSupportInfo
    description: Additional service support information for the MEC platform.
    type: object
    properties:
      type:
        required: false
        displayName: type
        description: Type of additional service support information for the MEC platform.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      data:
        required: false
        displayName: data
        description: Data about additional service support information for the MEC platform.
        type: MECPlatformsAdditionalSupportInfoData
  MECPlatformsAdditionalSupportInfoData:
    displayName: MECPlatformsAdditionalSupportInfoData
    description: Data about additional service support information for the MEC platform.
    type: object
    properties:
      additionalInfo:
        required: false
        displayName: additionalInfo
        type: string
        maxLength: 32
  ResourcesEdgeHostedService:
    displayName: ResourcesEdgeHostedService
    description: Edge hosted service represented by Service Endpoint definition.
    type: object
    properties:
      ern:
        required: false
        example:
          value: LASVEGAS_NV_WL
        displayName: ern
        description: Edge Resource Name. A string identifier for a set of edge resources.
        type: string
        pattern: ^[A-Za-z0-9-_]{3,32}$
        maxLength: 32
      serviceEndpoint:
        required: false
        displayName: serviceEndpoint
        description: Service Endpoint path, address, and port.
        type: ResourcesServiceEndpoint
      applicationServerProviderId:
        required: false
        example:
          value: AWS
        displayName: applicationServerProviderId
        description: Unique ID representing the Edge Application Provider.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      applicationId:
        required: false
        example:
          value: IogspaceJan15
        displayName: applicationId
        description: Unique ID representing the Edge Application.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      serviceDescription:
        required: false
        example:
          value: ThieIt
        displayName: serviceDescription
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
  ResourcesEdgeHostedServiceWithProfileId:
    displayName: ResourcesEdgeHostedServiceWithProfileId
    description: Edge hosted service represented by Service Endpoint definition.
    type: object
    properties:
      ern:
        required: false
        example:
          value: us-east-1-wl1-atl-wlz-1
        displayName: ern
        description: Edge Resource Name. A string identifier for a set of edge resources.
        type: string
        pattern: ^[A-Za-z0-9-]{3,32}$
        maxLength: 32
      serviceEndpoint:
        required: false
        displayName: serviceEndpoint
        description: Service Endpoint path, address, and port.
        type: ResourcesServiceEndpoint
      applicationServerProviderId:
        required: false
        example:
          value: AWS
        displayName: applicationServerProviderId
        description: Unique ID representing the Edge Application Provider.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      applicationId:
        required: false
        example:
          value: IogspaceJan15
        displayName: applicationId
        description: Unique ID representing the Edge Application.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      serviceDescription:
        required: false
        example:
          value: ThieIt
        displayName: serviceDescription
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      serviceProfileID:
        required: false
        example:
          value: 4054ea9a-593e-4776-b488-697b1bfa4f3b
        displayName: serviceProfileID
        description: The system assigned ID of the service profile.
        type: string
  ResourcesServiceEndpoint:
    displayName: ResourcesServiceEndpoint
    description: Service Endpoint path, address, and port.
    type: object
    properties:
      URI:
        required: false
        example:
          value: http://base_path/some_segment/id
        displayName: URI
        description: URI of Service Endpoint if available.
        type: string
        pattern: '\w+:(/?/?)[^\s]+'
        maxLength: 32
      FQDN:
        required: false
        example:
          value: thingtest.verizon.com
        displayName: FQDN
        description: FQDN of Service Endpoint if available.
        type: string
        pattern: ^[A-Za-z0-9.]{3,32}$
        maxLength: 32
      IPv4Address:
        required: false
        example:
          value: 192.168.11.10
        displayName: IPv4Address
        description: IPv4 Address of Service Endpoint if available.
        type: string
        maxLength: 32
      IPv6Address:
        required: false
        example:
          value: 2001:0db8:85a3:0000:0000:8a2e:0370:1234
        displayName: IPv6Address
        description: IPv6 Address of Service Endpoint if available.
        type: string
        maxLength: 64
      port:
        required: false
        example:
          value: 1234
        displayName: port
        description: Port information of Service Endpoint if IPv4 or IPv6 is mentioned.
        type: integer
        minimum: 1
        maximum: 10000
        format: int32
  Region:
    displayName: Region
    description: Information representing a certain geographical or logical area where MEC resources and services are provided.
    type: object
    properties:
      regionId:
        required: false
        example:
          value: consectetur
        displayName: regionId
        description: The unique identifier of the region.
        type:
        - string
        - nil
      name:
        required: false
        example:
          value: US_EAST_1
        displayName: name
        description: A relevant and identifiable region name.
        type:
        - string
        - nil
        pattern: ^[A-Za-z0-9_]{3,32}$
        maxLength: 32
      countryCode:
        required: false
        example:
          value: nr
        displayName: countryCode
        description: A two-character alpha code for a country, based on ISO 3166-1 alpha-2. This is future functionality, so the value returned is 'null'.
        type:
        - string
        - nil
        pattern: ^[A-Za-z0-9]{2,3}$
        maxLength: 3
      metro:
        required: false
        example:
          value: e1D
        displayName: metro
        description: The metropolitan area or 'City' value.  This is future functionality, so the value returned is 'null'.
        type:
        - string
        - nil
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      area:
        required: false
        example:
          value: IdUESF
        displayName: area
        description: This is a sub-set of 'City' and is similar to 'Zone'. This is future functionality, so the value returned is 'null'.
        type:
        - string
        - nil
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
  MECPlatformResource:
    displayName: MECPlatformResource
    description: 5G Edge platform used for deployment of a service. The ern of each platform contains the name of the matching AWS Wavelength zone that can be used when deploying.
    type: object
    properties:
      ern:
        required: false
        example:
          value: 5x4VBwmfZbzSL3
        displayName: ern
        description: Edge Resource Name. A string identifier for a set of edge resources.
        type: string
        pattern: ^[A-Za-z0-9-]{3,32}$
        maxLength: 32
      zone:
        required: false
        example:
          value: e5oV52kMGjDLhnJSsLJZL
        displayName: zone
        description: Unique identifier representing a zone. *Note:* This will have a null value as a placeholder.
        type:
        - string
        - nil
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      region:
        required: false
        example:
          value: US_WEST_2
        displayName: region
        description: MEC region name. Current valid values are US_WEST_2 and US_EAST_1.
        type: string
      status:
        required: false
        default: unknown
        displayName: status
        description: Status of the MEC Platform (default is 'unknown')
        type: string
        enum:
        - Active
        - Inactive
        - unknown
      properties:
        required: false
        displayName: properties
        type: any
  NetworkResourcesType:
    displayName: NetworkResourcesType
    description: Network resources of a service profile.
    type: object
    properties:
      minBandwidthKbits:
        required: false
        example:
          value: 1
        displayName: minBandwidthKbits
        description: Minimum required connection bandwidth in Kbit/s for the application.
        type: integer
        minimum: 1
        maximum: 10000
        format: int32
      serviceContinuitySupport:
        required: false
        example:
          value: true
        displayName: serviceContinuitySupport
        description: Indicates if service continuity support is required or not for the application.
        type: boolean
      maxRequestRate:
        required: false
        example:
          value: 15
        displayName: maxRequestRate
        description: Maximum request rate that the application can handle.
        type: integer
        minimum: 1
        maximum: 100
        format: int32
      maxLatencyMs:
        required: true
        example:
          value: 20
        displayName: maxLatencyMs
        description: 'Maximum response time or latency that the application can handle, in milliseconds. Note: this value must be in multiples of 5.'
        type: integer
        minimum: 5
        maximum: 1000
        format: int32
      minAvailability:
        required: false
        example:
          value: 1
        displayName: minAvailability
        description: Minimum availability required for the server.
        type: integer
        minimum: 1
        maximum: 100
        format: int32
  ComputeResourcesType:
    displayName: ComputeResourcesType
    description: Compute resources of a service profile.
    type: object
    properties:
      GPU:
        required: false
        displayName: GPU
        description: GPU resources of a service profile.
        type: GPU
      minRAMGB:
        required: false
        example:
          value: 1
        displayName: minRAMGB
        description: Minimum RAM required in Gigabytes.
        type: integer
        minimum: 1
        maximum: 100
        format: int32
      minStorageGB:
        required: false
        example:
          value: 1
        displayName: minStorageGB
        description: Minimum storage requirement in Gigabytes.
        type: integer
        minimum: 1
        maximum: 100
        format: int32
  GPU:
    displayName: GPU
    description: GPU resources of a service profile.
    type: object
    properties:
      minCoreClockMHz:
        required: false
        example:
          value: 1
        displayName: minCoreClockMHz
        description: Minimum Core Clock value in megahertz.
        type: integer
        minimum: 1
        maximum: 10000
        format: int32
      minMemoryClockMHz:
        required: false
        example:
          value: 35740
        displayName: minMemoryClockMHz
        description: Minimum Memory Clock value in megahertz.
        type: integer
        minimum: 1
        maximum: 100000
        format: int32
      minBandwidthGBs:
        required: false
        example:
          value: 588
        displayName: minBandwidthGBs
        description: Minimum GPU bandwidth in GB/s.
        type: integer
        minimum: 1
        maximum: 1000
        format: int32
      minTFLOPS:
        required: false
        example:
          value: 33
        displayName: minTFLOPS
        description: Minimum Floating Point Operations Per Second in Teraflops.
        type: integer
        minimum: 1
        maximum: 100
        format: int32
  EdgeDiscoveryResult:
    displayName: EdgeDiscoveryResult
    description: Base type for all errors.
    type: object
    properties:
      status:
        required: true
        displayName: status
        description: HTTP status code or status of response.
        type: string
        pattern: ^[A-Za-z0-9]{3,32}$
        maxLength: 32
      message:
        required: true
        displayName: message
        description: Error details.
        type: string
        maxLength: 32
      data:
        required: false
        displayName: data
        description: For cases where user input exceeds the boundary values an additional 'data' key will be returned with a relevant description.
        type: EdgeDiscoveryResultData
  EdgeDiscoveryResultData:
    displayName: EdgeDiscoveryResultData
    description: For cases where user input exceeds the boundary values an additional 'data' key will be returned with a relevant description.
    type: object
    properties:
      additionalMessage:
        required: false
        displayName: additionalMessage
        type: string
        maxLength: 32
  Account:
    example:
      value:
        accountName: Chintan_CPNStaticBulk
        accountNumber: 1234567890-77777
        organizationName: ChintanCPNBulk
        isProvisioningAllowed: true
        carriers:
        - Verizon Wireless
        features:
        - Static IP
        - Dynamic IP
        - Customer PN
        iPPools:
        - poolName: ACMESTATIC001
          poolType: Static IP
          isDefaultPool: true
        - poolName: ACMEDYNAMIC001
          poolType: Dynamic IP
          isDefaultPool: false
        servicePlans:
        - name: ''
          code: '92876'
          sizeKb: 1
          carrierServicePlanCode: ''
          extendedAttributes: []
        - name: ''
          code: '92876'
          sizeKb: 1
          carrierServicePlanCode: ''
          extendedAttributes: []
    displayName: Account
    description: Returns information about a specified account.
    type: object
    properties:
      accountName:
        required: false
        example:
          value: Chintan_CPNStaticBulk
        displayName: accountName
        description: The name of the account.
        type: string
      accountNumber:
        required: false
        example:
          value: 1234567890-77777
        displayName: accountNumber
        description: The billing number of the account.
        type: string
      organizationName:
        required: false
        example:
          value: ChintanCPNBulk
        displayName: organizationName
        description: The name of the organization that the account is part of.
        type: string
      isProvisioningAllowed:
        required: false
        example:
          value: true
        displayName: isProvisioningAllowed
        description: True if devices can be added to the account and activated with a single request. False if devices must be added to the account before they can be activated.
        type: boolean
      carriers:
        required: false
        example:
          value:
          - Verizon Wireless
        displayName: carriers
        description: The names of all carriers for the account.
        type: array
        items:
          type: string
      features:
        required: false
        example:
          value:
          - Static IP
          - Dynamic IP
          - Customer PN
        displayName: features
        description: The names of features that are enabled for the account.
        type: array
        items:
          type: string
      iPPools:
        required: false
        example:
          value:
          - poolName: ACMESTATIC001
            poolType: Static IP
            isDefaultPool: true
          - poolName: ACMEDYNAMIC001
            poolType: Dynamic IP
            isDefaultPool: false
