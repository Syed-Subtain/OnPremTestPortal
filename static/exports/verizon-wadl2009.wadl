          </xs:element>
          <xs:element minOccurs="0" name="hasMoreData" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>False for a status 200 response.True for a status 202 response, indicating that there is more data to be retrieved. Send another request, adjusting the earliest value in the request based on the occuredAt value for the last device in the current response.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="ContactInfoUpdateRequest">
