          <xs:enumeration value="Vehicle" />
          <xs:enumeration value="VulnerableRoadUser" />
          <xs:enumeration value="TrafficLightController" />
          <xs:enumeration value="InfrastructureSensor" />
          <xs:enumeration value="OnboardSensor" />
          <xs:enumeration value="Software" />
