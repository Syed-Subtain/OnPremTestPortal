            </xs:simpleType>
          </xs:element>
          <xs:element minOccurs="1" name="key.pem">
            <xs:simpleType>
              <xs:annotation>
                <xs:documentation>The string containing the private key</xs:documentation>
              </xs:annotation>
              <xs:restriction base="xs:string">
                <xs:pattern value="^[a-zA-Z0-9~\+\-!@#$%^&amp;*()\`\[\]{=};\&quot;':,.\/&lt;&gt;?|\s]+$" />
                <xs:maxLength value="4096" />
                <xs:minLength value="0" />
