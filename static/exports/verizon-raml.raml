                "enum": [
                  "0",
                  "1",
                  "2"
                ]
              },
              "deviceList": {
                "description": "Device list.",
                "example": [
                  {
                    "id": "980003420535573",
                    "kind": "imei",
                    "mdn": "7892345678"
                  },
                  {
                    "id": "375535024300089",
                    "kind": "imei",
                    "mdn": "7897654321"
                  },
                  {
                    "id": "A100003861E585",
                    "kind": "meid",
                    "mdn": "7897650914"
                  }
                ],
                "type": "array",
                "items": {
                  "id": "DeviceInfo",
                  "title": "DeviceInfo",
                  "description": "The devices that you want to locate. The array cannot contain more than 20 devices.",
                  "example": {
                    "id": "980003420535573",
                    "kind": "imei",
                    "mdn": "7892345678"
                  },
                  "type": "object",
                  "properties": {
                    "id": {
                      "description": "Device identifier.",
                      "example": "980003420535573",
                      "type": "string"
                    },
                    "kind": {
                      "description": "Device identifier kind.",
                      "example": "imei",
                      "type": "string"
                    },
