            "title": "LocationRequest",
            "description": "Request to obtain location of devices.",
            "example": {
              "accountName": "1234567890-00001",
              "accuracyMode": "0",
              "cacheMode": "1",
              "deviceList": [
                {
                  "id": "980003420535573",
                  "kind": "imei",
                  "mdn": "7892345678"
                },
                {
                  "id": "375535024300089",
                  "kind": "imei",
                  "mdn": "7897654321"
                },
                {
                  "id": "A100003861E585",
                  "kind": "meid",
                  "mdn": "7897650914"
                }
              ]
            },
            "type": "object",
            "properties": {
              "accountName": {
                "description": "Account identifier in \"##########-#####\".",
                "example": "1234567890-00001",
                "type": "string"
              },
              "accuracyMode": {
                "title": "AccuracyMode",
                "description": "Accurary, currently only 0-coarse supported.",
                "example": "0",
